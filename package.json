{
  "private": true,
  "name": "@janghye0k/datepicker",
  "version": "0.0.0",
  "description": "Javascript datepicker",
  "main": "datepicker.js",
  "types": "index.d.ts",
  "module": "index.esm.js",
  "exports": {
    ".": {
      "import": "./index.esm.js",
      "browser": "./datepicker.js"
    }
  },
  "keywords": [
    "vanilla",
    "datepicker",
    "calendar"
