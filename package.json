    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^5.0.2",
