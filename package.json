    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.1.6",
    "@types/animejs": "^3.1.12",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
