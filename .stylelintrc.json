{
  "plugins": ["stylelint-prettier"],
  "extends": ["stylelint-config-recommended"],
  "overrides": [
    {
      "files": ["**/*.scss"],
      "customSyntax": "postcss-scss"
    }
  ],
  "rules": {
    "prettier/prettier": true,
    "selector-max-compound-selectors": null,
    "color-named": null
  }
}
